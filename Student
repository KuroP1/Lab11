public class Student{
    private String name;
    private int id;
    private double score;
    public String getName(){return name;}        // get name
    public void setName(String n){name = n;}     //setName
    
    public int getId(){return id;}               //getId
    public void setId(int i){id = i;}            //setId
    
    public double getScore(){return score;}      //getScore
    public void setScore(double s) {score = s;}  //setScore    
}
